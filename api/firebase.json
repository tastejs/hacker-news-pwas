{
  "hosting": {
    "public": "dist/public",
    "rewrites": [
      {
        "source": "/v0/**",
        "function": "api0"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
}
